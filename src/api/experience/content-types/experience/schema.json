{
  "kind": "collectionType",
  "collectionName": "experiences",
  "info": {
    "singularName": "experience",
    "pluralName": "experiences",
    "displayName": "Experience",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "JobTitle": {
      "type": "string",
      "default": "Ex:Software Developer",
      "required": true
    },
    "CompanyName": {
      "type": "string",
      "required": true
    },
    "DateStart": {
      "type": "date"
    },
    "FinishDate": {
      "type": "date"
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "Company"
    },
    "skills": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::skill.skill",
      "mappedBy": "experiences"
    },
    "programming_languages": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::programming-language.programming-language",
      "mappedBy": "experiences"
    },
    "Current": {
      "type": "boolean"
    },
    "JopDescription": {
      "type": "blocks"
    },
    "CompanyLogo": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
